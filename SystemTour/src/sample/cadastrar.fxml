<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.CadastrarController">
   <children>
      <Pane layoutY="-1.0" prefHeight="300.0" prefWidth="380.0">
         <children>
            <Label layoutX="133.0" layoutY="2.0" prefHeight="37.0" prefWidth="133.0" text="Cadastrar Cliente">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Pane layoutY="51.0" prefHeight="215.0" prefWidth="380.0">
               <children>
                  <Label contentDisplay="RIGHT" layoutX="105.0" layoutY="2.0" prefHeight="25.0" prefWidth="210.0" text="Nome:">
                     <graphic>                       
                        <TextField fx:id="nomeCadastro" prefHeight="25.0" prefWidth="119.0" />
                     </graphic></Label>
                  <Label contentDisplay="RIGHT" layoutX="105.0" layoutY="35.0" prefHeight="25.0" prefWidth="188.0" text="Email:">
                     <graphic>
                        <TextField fx:id="emailCadastro" prefHeight="25.0" prefWidth="125.0" />
                     </graphic></Label>
                  <Label contentDisplay="RIGHT" layoutX="103.0" layoutY="68.0" prefHeight="25.0" prefWidth="170.0" text="CPF:">
                     <graphic>
                        <TextField fx:id="cpfCadastro" prefHeight="25.0" prefWidth="137.0" />
                     </graphic></Label>
                  <Label contentDisplay="RIGHT" layoutX="102.0" layoutY="101.0" prefHeight="25.0" prefWidth="188.0" text="Idade:">
                     <graphic>
                        <TextField fx:id="idadeCadastro" prefHeight="25.0" prefWidth="130.0" />
                     </graphic></Label>
                  <Label contentDisplay="RIGHT" layoutX="99.0" layoutY="135.0" text="Telefone:">
                     <graphic>
                        <TextField fx:id="telefoneCadastro" prefHeight="25.0" prefWidth="118.0" />
                     </graphic></Label>
                  <Button layoutX="173.0" layoutY="176.0" mnemonicParsing="false" onAction="#buttonAction" text="Cadastrar" />
               </children></Pane>
         </children></Pane>
   </children>
</AnchorPane>
