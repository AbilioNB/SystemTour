<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="253.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.VenderPacoteController">
   <children>
      <TabPane prefHeight="253.0" prefWidth="330.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Vender Pacote">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="14.0" layoutY="14.0" nodeOrientation="RIGHT_TO_LEFT" text="Valor Despesa">
                           <graphic>
                              <TextField fx:id="valorDespesa" />
                           </graphic>
                        </Label>
                        <Label layoutX="14.0" layoutY="56.0" nodeOrientation="RIGHT_TO_LEFT" text="Taxa de Desconto">
                           <graphic>
                              <TextField fx:id="desconto" />
                           </graphic>
                        </Label>
                        <Label layoutX="14.0" layoutY="94.0" nodeOrientation="RIGHT_TO_LEFT" text="Quantidade de Clientes">
                           <graphic>
                              <TextField fx:id="qtdClientes" />
                           </graphic>
                        </Label>
                        <Label layoutX="14.0" layoutY="138.0" nodeOrientation="RIGHT_TO_LEFT" text="CPF do Titular">
                           <graphic>
                              <TextField fx:id="cpfTitular" />
                           </graphic>
                        </Label>
                        <Button layoutX="261.0" layoutY="178.0" mnemonicParsing="false" onAction="#buttonEnviar" text="Vender" />
                        <Button layoutX="14.0" layoutY="178.0" mnemonicParsing="false" onAction="#buttonVoltar" text="Voltar" />
                        <Button layoutX="306.0" layoutY="8.0" mnemonicParsing="false" onAction="#buttonTestar" prefHeight="7.0" prefWidth="21.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
